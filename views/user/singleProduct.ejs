document.querySelectorAll('.btn-wishlist').forEach(button => {
    button.addEventListener('click', function(e) {
        e.preventDefault();
        
        const productId = this.getAttribute('data-product-id');
        console.log('productId', productId);

        // Determine if the product is already in the wishlist
        const isAdded = this.classList.toggle('added'); // Toggle 'added' class
        const fetchUrl = isAdded ? '/addToWishList' : '/removeWishList'; // Set URL based on action
        const actionMessage = isAdded ? "Product successfully added to wishlist" : "Product removed from wishlist"; // Action message

        // Change button color based on the action
        if (isAdded) {
            this.style.backgroundColor = 'pink'; // Change color to pink when added
        } else {
            this.style.backgroundColor = ''; // Reset color when removed
        }

        // Fetch request to add/remove the product
        fetch(fetchUrl, {
            method: "POST",
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify({ productId })
        })
        .then(response => response.json())
        .then(data => {
            console.log('data in wishlist', data);
            if (data.success) {
                // Display success message
                Swal.fire({
                    icon: 'success',
                    title: 'Success',
                    text: actionMessage,
                    confirmButtonText: "OK"
                });
            } else {
                // Display error message
                Swal.fire({
                    icon: 'error',
                    title: "Failed",
                    text: data.message,
                    confirmButtonText: "OK"
                });
            }
        })
        .catch(err => {
            // Handle error and display notification
            console.error('Error:', err);
            Swal.fire({
                icon: 'error',
                title: "Failed",
                text: "An error occurred while processing your request.",
                confirmButtonText: "OK"
            });
        });
    });
});


