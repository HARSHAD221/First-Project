                                <canvas id="myChart" height="120"></canvas>

                            </article>
                        </div>
                    </div>
                </div>

                <div class="product-category-tables">
                    <div>
                        <h5>Best Selling Products</h5>
                        <% if (topProducts.length > 0) { %>
                            <table class="table">
                                <thead><tr><th>Product</th><th>Quantity</th></tr></thead>
                                <tbody>
                                    <% topProducts.forEach(p => { %>
                                        <tr><td><%= p.productName %></td><td><%= p.totalSales %></td></tr>
                                    <% }) %>
                                </tbody>
                            </table>
                        <% } else { %>
                            <p>No top-selling products data</p>
                        <% } %>
                    </div>
                    <div>
                        <h5>Top Categories</h5>
                        <% if (topCategories.length > 0) { %>
                            <table class="table">
                                <thead>
                                    <tr><th>Category</th><th>Sales</th></tr>
                                </thead>
                                <tbody>
                                    <% topCategories.forEach(c => { %>
                                        <tr>
                                            <td><%= c._id %></td> <!-- This is the category name -->
                                            <td><%= c.totalSales %></td> <!-- Total sales for the category -->
                                        </tr>
                                    <% }) %>
                                </tbody>
                            </table>
                        <% } else { %>
                            <p>No category data available</p>
                        <% } %>
                    </div>
                    
                </div>
